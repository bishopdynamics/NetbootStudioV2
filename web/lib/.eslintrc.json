{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "google",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "indent": ["error", 4],
        "max-len": "off",
        "new-cap": "off",
        "camelcase": "off",
        "require-jsdoc": "off",
        "no-unused-vars": "off"
    }
}
